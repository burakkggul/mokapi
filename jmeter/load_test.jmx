            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Access Header Post Processor" enabled="true">
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="script">function getAccessKey() {
  var headers = prev.getResponseHeaders().split(&apos;\n&apos;);
  for (var i = 0; i &lt; headers.length; i++) {
    var header = headers[i].split(&apos;:&apos;);
    if (header[0].trim() === &apos;Access-Key&apos;) {
      accessKey = header[1];
      vars.put(&apos;accessKey&apos;, accessKey);
      log.info(&quot;accessKey:&quot; + accessKey);
      break;
    }
  }
}

log.info(&quot;Post Processor Begin&quot;);

var url = prev.getURL().toString();

if(url.indexOf(&quot;access-header&quot; &gt; -1)){
	log.info(url);
	getAccessKey();
	  
}
</stringProp>
              <stringProp name="scriptLanguage">javascript</stringProp>
            </JSR223PostProcessor>
